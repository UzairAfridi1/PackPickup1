@model IEnumerable<PackPickup1.Models.User>
    Layout = "~/Views/Shared/_LayoutLogin.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "New")
</p>
<table class="table table-bordered table-dark" >
    <tr>
        <th>
            Name
        </th>
        <th>
            Email Address
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Name
        </td>
        <td>
            @item.Email
        </td>
 
    </tr>
}

</table>
